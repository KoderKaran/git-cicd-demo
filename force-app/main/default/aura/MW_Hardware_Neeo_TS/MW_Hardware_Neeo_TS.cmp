<aura:component >
	<aura:registerEvent name="bubblingEvent" type="c:MW_Screen_Event" />   	

    <h1 class="slds-text-heading_large text-red">Troubleshooting</h1>
    
    <div class="slds-p-top_large slds-scrollable_none">
        
        <p class="text-paragraph bold">What error message do you see on your mobile device?</p>        
        
        <div class="slds-grid slds-wrap slds-p-top_x-large slds-grid_align-center slds-gutters">
          <div class="slds-col slds-p-bottom_large">
            <span style="cursor: pointer;" onclick="{!c.NeeoTSNEClick}">
            <img src="https://res.cloudinary.com/control4/image/upload/c_scale,w_220/customer-support/customer-support-app-errors-v1a-pg-copy_network-error.png"/>
            </span>
          </div>
          <div class="slds-col slds-p-bottom_large">
            <span style="cursor: pointer;" onclick="{!c.NeeoTSARClick}">
            <img src="https://res.cloudinary.com/control4/image/upload/c_scale,w_220/v1573172325/customer-support/customer-support-app-errors-v1a-pg-copy_authentication-required.png"/>
              </span>
          </div>
          <div class="slds-col slds-p-bottom_x-large">
            <span style="cursor: pointer;" onclick="{!c.NeeoTSOtherClick}">
            <img src="https://res.cloudinary.com/control4/image/upload/c_scale,w_220/customer-support/customer-support-app-errors-v1a-pg-copy_other.png"/>
            </span>
          </div>
        </div>
    </div>    
    <lightning:button aura:id="back" class="slds-button slds-button_neutral" label="Back" onclick="{!c.BackButtonClick}"/>
</aura:component>