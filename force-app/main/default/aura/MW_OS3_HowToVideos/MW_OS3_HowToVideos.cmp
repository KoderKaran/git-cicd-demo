<aura:component >
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />  
    <aura:registerEvent name="bubblingEvent" type="c:MW_Screen_Event" />  
    
    <aura:attribute name="activeSections" type="List" default="[]" />
    <div aura:id="Main_Div">
        <h1 class="slds-text-heading_large text-red">How-to Videos</h1>
        
        <div class="slds-p-top_large slds-m-bottom_small">
            <p class="text-paragraph bold">Use the videos below to familiarize yourself with the OS 3 interface as well as its capabilities and features.</p>
        </div>  
        
        
        <lightning:accordion allowMultipleSectionsOpen="true" onsectiontoggle="{! c.handleSectionToggle }" >       
            <lightning:accordionSection class="slds-text-heading_large text-red" name="Navigation" label="NAVIGATION">
                <aura:set attribute="body">
                    <li><a onclick="{!c.Vid5Click}" class="slds-text-link slds-p-left_xx-large text-paragraph" style="white-space: nowrap"><u>Using the Control4 Interface</u></a></li>
                   <!-- <li><a onclick="{!c.Vid18Click}" class="slds-text-link slds-p-left_xx-large text-paragraph" style="white-space: nowrap"><u>On-Screen Navigation</u></a></li>-->
                    
