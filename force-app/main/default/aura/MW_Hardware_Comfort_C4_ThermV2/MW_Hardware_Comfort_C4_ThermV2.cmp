<aura:component >
	<aura:registerEvent name="bubblingEvent" type="c:MW_Screen_Event" />  

    <h1 class="slds-text-heading_large text-red">Thermostat V2</h1>
    
    <div class="slds-p-top_large slds-scrollable_none">   
        <div class="slds-grid slds-grid_align-center slds-gutters_medium ">
          <div class="slds-col">
                <img src="https://res.cloudinary.com/control4/image/upload/e_trim/c_pad,w_600,h_350,f_auto,q_auto/customer-support/Thermostats_V2.png"/>
          </div>
        </div>
    </div>    
    
    <div class="slds-grid slds-wrap slds-p-top_x-large">
        <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-order_2 slds-medium-order_1">
            <lightning:button aura:id="back" class="slds-button slds-button_neutral" label="Back" onclick="{!c.BackButtonClick}"/>
        </div>
        <div class="slds-col slds-size--1-of-1 slds-medium-size--3-of-4 slds-order_1 slds-medium-order_2">
            <div class="slds-float_right">
            	<lightning:button aura:id="TroubleshootButton" class="slds-button style1button" label="Troubleshoot" onclick="{!c.TroubleshootClick}"/>
                <lightning:button aura:id="GuideButton" class="slds-button style1button" label="Guide" onclick="{!c.GuideClick}"/>
            </div>
        </div>
    </div>     
</aura:component>