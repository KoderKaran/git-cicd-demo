<aura:component >
	<aura:registerEvent name="bubblingEvent" type="c:MW_Screen_Event" />    
    
    <h1 class="slds-text-heading_large text-red">403.21 - Step 2</h1>
          
    <div class="slds-scrollable_none">      
        <div class="slds-p-top_large">
              <p class="slds-text-heading_small slds-text-title_bold">Select your mobile operating system.</p>              
          </div>
    </div>   

    <div class="slds-grid slds-wrap slds-p-top_x-large">
        <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-4 slds-order_2 slds-medium-order_1">
            <lightning:button aura:id="back" class="slds-button slds-button_neutral" label="Back" onclick="{!c.BackButtonClick}"/>
        </div>
        <div class="slds-col slds-size--1-of-1 slds-medium-size--3-of-4 slds-order_1 slds-medium-order_2">
            <div class="slds-float_right">
                <button class="slds-button style1button" onclick="{!c.iOSButtonClick}"><img src="https://res.cloudinary.com/control4/image/upload/customer-support/Operating_System_Logos_iOS.svg" />iOS</button>
                <button class="slds-button style1button" onclick="{!c.AndroidButtonClick}"><img src="https://res.cloudinary.com/control4/image/upload/customer-support/Operating_System_Logos_Android.svg" />Android</button>
            </div>
        </div>
    </div>       
</aura:component>