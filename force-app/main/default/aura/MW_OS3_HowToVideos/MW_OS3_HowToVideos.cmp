            <lightning:accordionSection class="slds-text-heading_large text-red" name="Customization" label="CUSTOMIZATION AND PERSONALIZATION">
                <aura:set attribute="body">
                    <li><a onclick="{!c.Vid9Click}" class="slds-text-link slds-p-left_xx-large text-paragraph" style="white-space: nowrap"><u>Favorite Devices and Media</u></a></li>
                    <li><a onclick="{!c.Vid20Click}" class="slds-text-link slds-p-left_xx-large text-paragraph" style="white-space: nowrap"><u>Set My Own Background</u></a></li>
                </aura:set>
            </lightning:accordionSection>
            
            <lightning:accordionSection class="slds-text-heading_large text-red" name="Entertainment" label="ENTERTAINMENT">
                <aura:set attribute="body">
                    <li><a onclick="{!c.Vid1Click}" class="slds-text-link slds-p-left_xx-large text-paragraph" style="white-space: nowrap"><u>Stream Music</u></a></li>
                    <li><a onclick="{!c.Vid16Click}" class="slds-text-link slds-p-left_xx-large text-paragraph" style="white-space: nowrap"><u>Pick a Radio Station</u></a></li>
                    <li><a onclick="{!c.Vid17Click}" class="slds-text-link slds-p-left_xx-large text-paragraph" style="white-space: nowrap"><u>Control Audio or Video That's Playing</u></a></li>
